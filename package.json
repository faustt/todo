{
	"name": "todo",
	"version": "0.0.0-dev",
	"private": true,
	"scripts": {
		"build": "./scripts/build.sh && yarn build:post",
		"build:post": "ts-node --project scripts/tsconfig.json --transpile-only scripts/postbuild",
		"dev": "./scripts/dev.sh",
		"format": "prettier --check .",
		"format:fix": "prettier --write .",
		"serve": "vite preview",
		"test": "ts-node-dev --transpile-only --respawn --project ./test/tsconfig.json ./test"
	},
	"devDependencies": {
		"@faustt/prettier-config": "^2.0.2",
		"@faustt/release": "^0.9.1",
		"@sveltejs/vite-plugin-svelte": "^1.0.0-next.7",
		"@types/glob": "^7",
		"@types/node": "^14",
		"@types/prettier": "^2",
		"@types/uuid": "^8",
		"autoprefixer": "^10.2.5",
		"cssnano": "^5.0.1",
		"dexie": "^3.0.3",
		"glob": "^7.1.7",
		"playwright": "^1.10.0",
		"postcss": "^8.2.10",
		"postcss-load-config": "^3.0.1",
		"prettier": "^2.2.1",
		"prettier-plugin-svelte": "^2.2.0",
		"svelte": "^3.37.0",
		"svelte-preprocess": "^4.7.1",
		"svelte-spa-router": "^3.1.0",
		"svex": "^1.0.0-alpha.1",
		"tailwindcss": "^2.2.7",
		"ts-node": "^9.1.1",
		"ts-node-dev": "^1.1.6",
		"typescript": "^4.2.4",
		"uuid": "^8.3.2",
		"vite": "^2.4.3",
		"vite-plugin-pwa": "^0.7.3"
	},
	"prettier": "@faustt/prettier-config"
}
